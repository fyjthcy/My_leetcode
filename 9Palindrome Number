class Solution {//负数没有回文数
public:
    bool isPalindrome(int x) {
        int div = 1;
        if(x<0) return false;
        while(div<=x/10)
        {
            div*=10;
        }
        //if(div==100) return false;
        while(x)
        {
            
            if(x/div!=x%10)
            {
                return false;
            }
            else
            {
                x=(x%div)/10;
                div=div/100;
            }
        }
        return true;
    }
};
